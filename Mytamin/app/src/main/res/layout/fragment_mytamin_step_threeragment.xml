<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/mytamin_frametwo"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:visibility="visible"
    >
    <TextView
        android:id="@+id/mytamin_frametwo_title_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="40dp"
        android:text="오늘의 마음 컨디션은\n어떤가요?"
        android:textSize="30sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_title_text"
        app:layout_constraintBottom_toTopOf="@+id/mytamin_frametwo_select_ImageView"
        android:layout_marginLeft="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/ic_baseline_calendar_today_24"
        android:id="@+id/mytamin_frametwo_calendar_btn"
        android:background="@color/background_white"
        android:padding="10dp"

        />
    <TextView
        android:layout_width="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_title_text"
        app:layout_constraintBottom_toTopOf="@+id/mytamin_frametwo_select_ImageView"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/mytamin_frametwo_calendar_btn"
        android:id="@+id/mytamin_frametwo_calendar_text"
        android:padding="10dp"
        android:text="9월 21일"
        />



    <ImageView
        android:id="@+id/mytamin_frametwo_select_ImageView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_title_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintBottom_toTopOf="@+id/mytamin_frametwo_state"
        />
    <TextView
        android:id="@+id/mytamin_frametwo_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_select_ImageView"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/mytamin_frametwo_linear"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mytamin_frametwo_linear"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_state"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <ImageButton
            android:id="@+id/mytamin_frametwo_image_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icons8ogre48"
            android:padding="10dp"
            android:background="@color/background_white"
            android:tag="5"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:id="@+id/mytamin_frametwo_image_two"
            android:layout_height="wrap_content"
            android:src="@drawable/icons8angryface48"
            android:padding="10dp"
            android:background="@color/background_white"
            android:tag="4"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mytamin_frametwo_image_three"
            android:src="@drawable/icons8facwithrollingeyes48"
            android:padding="10dp"
            android:tag="3"
            android:background="@color/background_white"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icons8facewithhandovemouth48"
            android:id="@+id/mytamin_frametwo_image_four"
            android:tag="2"
            android:padding="10dp"
            android:background="@color/background_white"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icons8grinningsquintingface48"
            android:id="@+id/mytamin_frametwo_image_five"
            android:tag="1"
            android:padding="10dp"
            android:background="@color/background_white"
            />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>