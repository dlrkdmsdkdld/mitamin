<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/mytamin_frametwo"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:visibility="visible"
    >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:text="@string/mytamin_step_four_title"
        android:fontFamily="@font/appmainfont_bold"
        android:includeFontPadding="false"
        android:textSize="24sp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="24dp"
        android:id="@+id/mytamin_step_three_title"
        android:textColor="@color/black"
        />
    <TextView
        android:id="@+id/mytamin_step_three_state"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="40dp"
        android:text="@string/mytamin_step_three_state"
        android:textSize="18sp"
        android:textColor="@color/darkGray"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_step_three_title"
        android:fontFamily="@font/appmainfont_medium"
        android:includeFontPadding="false"
        />




    <ImageView
        android:id="@+id/mytamin_frametwo_select_ImageView"
        android:layout_width="172dp"
        android:layout_height="172dp"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_step_three_state"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"

        />
    <TextView
        android:id="@+id/mytamin_frametwo_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_select_ImageView"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:includeFontPadding="false"
        android:fontFamily="@font/appmainfont_bold"
        android:textColor="@color/black"
        android:layout_marginTop="20dp"
        />
    <LinearLayout
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mytamin_frametwo_linear"
        app:layout_constraintTop_toBottomOf="@+id/mytamin_frametwo_state"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <ImageButton
            android:id="@+id/mytamin_frametwo_image_one"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@drawable/ic_so_bad_disabled"
            android:padding="10dp"
            android:tag="5"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            />
        <ImageButton
            android:layout_width="48dp"
            android:id="@+id/mytamin_frametwo_image_two"
            android:layout_height="48dp"
            android:background="@drawable/ic_bad_disbled"
            android:padding="10dp"
            android:layout_marginRight="20dp"
            android:tag="4"
            />
        <ImageButton
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:id="@+id/mytamin_frametwo_image_three"
            android:background="@drawable/ic_so_so_disabled"
            android:padding="10dp"
            android:layout_marginRight="20dp"
            android:tag="3"
            />
        <ImageButton
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@drawable/ic_good_disabled"
            android:id="@+id/mytamin_frametwo_image_four"
            android:tag="2"
            android:padding="10dp"
            android:layout_marginRight="20dp"
            />
        <ImageButton
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@drawable/ic_so_good_disabled"
            android:id="@+id/mytamin_frametwo_image_five"
            android:layout_marginRight="20dp"
            android:tag="1"
            android:padding="10dp"
            />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>